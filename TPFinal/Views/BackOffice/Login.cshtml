
@{
    
    @using (Html.BeginForm("Registro", "Backoffice", FormMethod.Post))
        {
        <p>Usuario:</p> @Html.TextBoxFor(model => model.Nombre) @Html.ValidationMessageFor(model => model.Nombre); <br />
        <p>Contraseña:</p> @Html.PasswordFor(model => model.Pass) @Html.ValidationMessageFor(model => model.Pass); <br />
        <input type="submit" value="Enviar" />
    }

        
}

<h2>Login</h2>

