@using TPFinal.Models
@model Noticias

@{
    <h2>Actualizar Noticias</h2>
}



@foreach (Noticias Not in ViewBag.ListaNoticias)
{

    <p> <a href="@Url.Action("Modificar","BackOffice",new { idnoticia= Not.IdNoticia })"> @Not.Titulo</a></p>
}

<input type="submit" value="Continuar" />



